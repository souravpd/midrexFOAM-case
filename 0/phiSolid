{
    outlet
    {
        type            calculated;
        value           uniform 0;
    }
    inlet2
    {
        type            calculated;
        value           uniform 0;
    }
    inlet1
    {
        type            calculated;
        value           uniform 0;
    }
    outletSolid
    {
        type            calculated;
        value           uniform 0;
    }
    coolingOutlet1
    {
        type            calculated;
        value           uniform 0;
    }
    coolingOutlet2
    {
        type            calculated;
        value           uniform 0;
    }
    fixedWalls
    {
        type            calculated;
        value           uniform 0;
    }
    frontAndBack
    {
        type            empty;
        value           nonuniform 0();
    }
}


