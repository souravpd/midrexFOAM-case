FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      USolid;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    frontAndBack
    {
        type            empty;
    }
    fixedWalls
    {
        type           noSlip;
     }
    outletSolid
    {
     type               zeroGradient;
    }

    inlet1
    {
       type           noSlip;
          }
    inlet2
    {
        type            noSlip; 
    }
    outlet
      {
        type            uniformFixedValue;
        uniformValue    constant (-10 0 0);
        value           uniform (-10 0 0);
    }
}

