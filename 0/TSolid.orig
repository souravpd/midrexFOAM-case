FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      TSolid;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 298;

boundaryField
{
    frontAndBack
    {
        type            empty;
    }
    fixedWalls
    {
        type          zeroGradient;
    }
    outletSolid
    {
        type           zeroGradient;
    }

    inlet1
    {
       type           zeroGradient;
    }
    inlet2
    {
        type           zeroGradient; 
    }
    coolingOutlet1
    {
       type           zeroGradient;
    }
    coolingOutlet2
    {
        type           zeroGradient; 
    }
    outlet
      {
         type            uniformFixedValue;
         uniformValue    constant 263;
         value           uniform 263;
    }
}

